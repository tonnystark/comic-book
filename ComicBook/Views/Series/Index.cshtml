@model ComicBook.Models.Series[]
@{
    ViewBag.Title = "Series";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>
<div class="row">
    <div class="col-md-12">
        <ul class="series-list">
            @foreach (var item in Model)
            {
                <li>
                    <h4>@Html.ActionLink(@item.Title, "Detail", new { id = @item.Id })</h4>
                </li>
            }

        </ul>
    </div>
</div>
